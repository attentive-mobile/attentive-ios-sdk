# Fastlane Environment Variables (Default)
# Copy this file to .env for local development
# DO NOT commit .env file - it should contain your actual credentials

# ========================
# App Configuration
# ========================
APP_IDENTIFIER=com.attentive.sdk.example

# ========================
# Code Signing (Fastlane Match)
# ========================
# Git repository URL for storing certificates
# Example: git@github.com:your-org/certificates.git
MATCH_GIT_URL=

# Password for decrypting certificates
MATCH_PASSWORD=

# Apple ID for accessing Developer Portal
APPLE_ID=

# ========================
# App Store Connect
# ========================
# API Key ID (e.g., ABC123XYZ)
APP_STORE_CONNECT_API_KEY_ID=

# Issuer ID (UUID format)
APP_STORE_CONNECT_ISSUER_ID=

# API Key content (base64 encoded .p8 file)
# To encode: cat AuthKey_ABC123XYZ.p8 | base64
APP_STORE_CONNECT_API_KEY_CONTENT=

# ========================
# Fastlane Configuration
# ========================
FASTLANE_SKIP_UPDATE_CHECK=YES
FASTLANE_HIDE_CHANGELOG=YES
