# EditorConfig for attentive-ios-sdk
# https://editorconfig.org

root = true

[*]
charset = utf-8
end_of_line = lf
insert_final_newline = true
trim_trailing_whitespace = true

# Swift files
[*.swift]
indent_style = space
indent_size = 4

# Objective-C files
[*.{m,mm,h}]
indent_style = space
indent_size = 2

# Property lists
[*.plist]
indent_style = tab

# JSON/YAML
[*.{json,yml,yaml}]
indent_style = space
indent_size = 2

# Markdown
[*.md]
trim_trailing_whitespace = false
